package test_11;

import java.util.ArrayList;

public class Actor {
	
	public Actor(String names){
		
		name=names;
		
	}
	
	public void addRevenue(float f) {
		
		revenue.add(f);
		movies++;
		
	}
	
	public float avgRevenue() {
		
		return revenue/movies;
		
	}
	
	public float getrevenue(){
		
		return revenue;
	}
	
	String name;
	ArrayList<Float> revenue;
	int movies=0;

}
